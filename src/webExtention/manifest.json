{
    "manifest_version": 3,
    "name": "Fin checker",
    "description": "Extensión para verificar la seguridad de una página web.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "http://127.0.0.1:5000/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon4.png"
    }
   
}
